int x = 1;
int LED = 8;
int y;
void setup()
{
    pinMode(y,OUTPUT);
    pinMode(x,OUTPUT);
     while (x < 4){
       for (int y = 0; y < 256;y++){
        analogWrite (5,y);
        delay (4);
    }
        analogWrite ((LED+x),255);
        delay(2000);
    for(int y = 255; y >= 0;y--){
        analogWrite (5,y);
        delay(4);
    }
       x++;
          }
   
}

void loop()
{

}
